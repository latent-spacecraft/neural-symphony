{
  "name": "neural-symphony",
  "version": "1.0.0",
  "description": "AI Reasoning Orchestrator - Interactive visual interface for GPT-oss-20b chain-of-thought reasoning",
  "main": "src/backend/server.js",
  "scripts": {
    "start": "node src/backend/server.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon src/backend/server.js",
    "dev:frontend": "cd src/frontend && npm start",
    "build": "cd src/frontend && npm run build",
    "test": "jest",
    "lint": "eslint src/",
    "setup:model": "node scripts/setup-model.js",
    "demo": "node demos/run-demo.js"
  },
  "keywords": ["ai", "gpt-oss", "reasoning", "visualization", "openai", "hackathon"],
  "author": "Geoff Taghon",
  "license": "Apache-2.0",
  "dependencies": {
    "express": "^4.18.0",
    "ws": "^8.13.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "axios": "^1.4.0",
    "uuid": "^9.0.0",
    "dotenv": "^16.3.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "concurrently": "^9.0.1",
    "jest": "^29.7.0",
    "@types/node": "^22.9.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}